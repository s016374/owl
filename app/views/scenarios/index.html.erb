<tbody>
  <div class="col-md-12">
    <div class="page-header">
      <h2>
        <%= @project.title %> Cases
      </h2>
    </div>
    <%= render 'scenarios/table', scenarios: @scenarios, project: @project %>
  </div>
<br>
<div class="row">
  <div class="col-md-12">
    <div class="btn-group">
      <%= link_to icon_tag('glyphicon-chevron-left') + ' Projects', projects_path, class: "btn btn-default" %>
      <% if policy(current_user).new? %>
        <%= link_to icon_tag('glyphicon-plus') + ' New Scenario', new_project_scenario_path, class: "btn btn-default" %>
      <% end %>
      <%= link_to icon_tag('glyphicon-refresh') + ' Run All Scenarios', go_project_scenarios_path, class: "btn btn-default" %>
    </div>
  </div>
</div>
